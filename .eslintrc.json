{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "eslint-config-prettier"],
  "plugins": ["eslint-plugin-prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "all",
        "endOfLine": "auto",
        "semi": true,
        "printWidth": 100
      }
    ],
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": false
      }
    ],
    "no-unused-vars": 0,
    "no-use-before-define": 0,
    "no-shadow": 0,
    "consistent-return": 0
  }
}
